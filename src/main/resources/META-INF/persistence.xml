    <class>com.trouble.entities.Invoices</class>
    <class>com.trouble.entities.Labels</class>
    <class>com.trouble.entities.Reviews</class>
    <class>com.trouble.entities.Rss</class>
    <class>com.trouble.entities.Songs</class>
    <class>com.trouble.entities.SongwriterSongs</class>
    <class>com.trouble.entities.Songwriters</class>
    <class>com.trouble.entities.SurveyChoices</class>
    <class>com.trouble.entities.SurveyQuestions</class>
